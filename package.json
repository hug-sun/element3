{
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "vitest",
    "test:once": "vitest run",
    "lint": "eslint './**/*.{ts,tsx,js,vue,md}'",
    "lint:fix": "eslint --fix './**/*.{ts,tsx,js,vue,md}'",
    "coverage": "vitest run --coverage",
    "docs": "vitepress dev src/",
    "docs:build": "vitepress build src/",
    "docs:serve": "vitepress serve src/",
    "prepare": "husky install",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.6.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.11.3",
    "chalk": "^4.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.2",
    "sass": "^1.52.3",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitepress": "1.0.0-alpha.1",
    "vitepress-theme-demoblock": "^1.4.2",
    "vitest": "^0.15.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json,md,yml,yaml}": [
      "eslint --fix"
    ]
  }
}
